class Solution:
    def longestConsecutive(self, nums: List[int]) -> int:
        s=set(nums)
        m=0
        for i in s:
                maxi=0
                if i-1 not in s:
                    cur=i
                    maxi=1
                    while cur+1 in s:
                        cur+=1
                        maxi+=1
                    m=max(m,maxi)
        return m
        
        
        
